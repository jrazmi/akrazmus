{
  "name": "apollo-next-auth-starter-backend",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "dev": "yarn migrate-up && backpack",
    "dev-down": "yarn migrate-down && yarn migrate-up && backpack",
    "build": "rimraf build/ && backpack build",
    "test": "NODE_ENV=test LOG_LEVEL=critical yarn migrate-down && NODE_ENV=test LOG_LEVEL=critical yarn migrate-up --env test && LOG_LEVEL=critical jest --detectOpenHandles --runInBand --forceExit",
    "migrate-down": "knex-migrate down --to 0",
    "migrate-up": "knex-migrate up",
    "start": "node ./build/main.js"
  },
  "dependencies": {
    "apollo-server": "^2.6.9",
    "apollo-server-express": "^2.7.0",
    "aws-sdk": "^2.495.0",
    "backpack-core": "^0.8.4",
    "bcryptjs": "^2.4.3",
    "connect": "^3.7.0",
    "dataloader": "^1.4.0",
    "dotenv": "^8.0.0",
    "express-jwt": "^5.3.1",
    "graphql": "^14.4.2",
    "graphql-middleware": "^3.0.2",
    "graphql-shield": "^6.0.4",
    "graphql-yoga": "^1.18.1",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.19.0",
    "knex-migrate": "^1.7.2",
    "merge-graphql-schemas": "^1.5.8",
    "pg": "^7.11.0",
    "qs-middleware": "^1.0.3"
  },
  "devDependencies": {
    "eslint": "^6.0.1",
    "eslint-plugin-jest": "^22.7.2",
    "rimraf": "^2.6.3"
  }
}
